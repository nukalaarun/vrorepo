    <display-name><![CDATA[getProperties]]></display-name>
    <script encoded="false"><![CDATA[var Class = System.getModule("com.vmware.pscoe.library.class").Class();
var ResourceElementAccessor = Class.load("com.vmware.pscoe.library.util", "ResourceElementAccessor");
var res = new ResourceElementAccessor("RBA v8/MigrationMapping", "VM Properties Mapping.json", false);
var propsMapping = JSON.parse(res.getContent());
//System.log("REG__VRA_Set_VMProp_EB / dePayloadPre propsMapping: " + JSON.stringify(propsMapping, null, 4));
var machine = System.getModule("com.vmware.pscoe.rba.migration").getDeploymentVm();

var vmName = machine.name;
var vmId = machine.id;
System.log("VMId: " + vmId);

var vra7Vm = System.getModule("com.vmware.pscoe.rba.migration").getVra7Vm(vra7RestHost, vmName, vra7Tenant, vra7Username, vra7Password);
var vRA7DepName = vra7Vm.parentResourceRef.label;
System.log("vRA7DepName: " + vRA7DepName);


outputProps = {};
for (var propIndex in Object.keys(propsMapping)) {
    var vra7Prop = Object.keys(propsMapping)[propIndex];
    for (vmPropIndex in vra7Vm.resourceData.entries) {
        var prop = vra7Vm.resourceData.entries[vmPropIndex];
        if (prop.key == vra7Prop) {
            outputProps[propsMapping[vra7Prop]] = prop.value.value;
        }
    }
}

System.log("The following Log must be filled with vRA7 VM Data. Uncomment Line 5 and line 16 if necessary to verify");
System.log("Output props: " + JSON.stringify(outputProps, null, 4));







