  <workflow-item name="item2" out-name="item6" type="task">
    <display-name><![CDATA[Set New VM Name]]></display-name>
    <script encoded="false"><![CDATA[resourceNames = []
if(inputProperties.customProperties.machinePrefixName != undefined)
{
    var machinePrefixName = inputProperties.customProperties.machinePrefixName
}
else
{
    throw "Unable to retrieve machine prefix"
}




for each(var resource in inputProperties.resourceNames)
{
    if(ceMachinePrefix.getAttributeWithKey(machinePrefixName.toLowerCase()) == null)
    {
        var prefixNumber = 1
    }
    else
    {
        var prefixNumber = ceMachinePrefix.getAttributeWithKey(machinePrefixName.toLowerCase()).value
        prefixNumber++
    }
    var numString = prefixNumber.toString()
    var numLength = numString.length
    var gapNumber = maximumDigits - numLength
    for(var idx = 0; idx < gapNumber; idx++)
    {
        numString = "0" + numString
    }
    ceMachinePrefix.setAttributeWithKey(machinePrefixName.toLowerCase(),prefixNumber,"Number")
    resourceNames.push(machinePrefixName + numString)
}



]]></script>
    <in-binding>
      <bind name="maximumDigits" type="number" export-name="maximumDigits"/>
      <bind name="inputProperties" type="Properties" export-name="inputProperties"/>
      <bind name="ceMachinePrefix" type="ConfigurationElement" export-name="ceMachinePrefix"/>
    </in-binding>
    <out-binding>
